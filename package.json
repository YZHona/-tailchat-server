{
  "name": "pawchat-server",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/pawchat/pawchat-server.git",
  "author": "moonrailgun <moonrailgun@gmail.com>",
  "license": "GPLv3",
  "private": true,
  "scripts": {
    "dev": "ts-node ./node_modules/moleculer/bin/moleculer-runner.js --hot --repl --config moleculer.config.ts services/**/*.service.ts",
    "start:comet": "ts-node ./packages/comet/index.ts",
    "start:logic": "ts-node ./packages/logic/index.ts",
    "protobuf": "proto-loader-gen-types --longs=String --enums=String --defaults --oneofs --grpcLib=@grpc/grpc-js --outDir=proto/ proto/*.proto"
  },
  "dependencies": {
    "async-exit-hook": "^2.0.1",
    "config": "^3.3.6",
    "etcd3": "^1.1.0",
    "ioredis": "^4.27.6",
    "jsonwebtoken": "^8.5.1",
    "moleculer": "^0.14.13",
    "moleculer-db": "^0.8.14",
    "moleculer-db-adapter-sequelize": "^0.2.11",
    "moleculer-web": "^0.10.0-beta2",
    "prettier": "^2.3.1",
    "sequelize": "^6.6.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.3"
  },
  "devDependencies": {
    "@types/config": "^0.0.38",
    "@types/jsonwebtoken": "^8.5.2",
    "moleculer-repl": "^0.6.5"
  }
}
